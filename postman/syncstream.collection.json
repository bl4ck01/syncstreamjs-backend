        {
          "name": "Signup",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test(\"Status code is 200\", function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "",
                  "pm.test(\"Response has success true\", function () {",
                  "    const jsonData = pm.response.json();",
                  "    pm.expect(jsonData.success).to.eql(true);",
                  "});",
                  "",
                  "pm.test(\"User ID is returned\", function () {",
                  "    const jsonData = pm.response.json();",
                  "    pm.expect(jsonData.data).to.have.property('id');",
                  "    pm.collectionVariables.set(\"userId\", jsonData.data.id);",
                  "});",
                  "",
                  "// Extract auth token from cookie",
                  "const cookie = pm.response.headers.get('Set-Cookie');",
                  "if (cookie) {",
                  "    const authMatch = cookie.match(/auth=([^;]+)/);",
                  "    if (authMatch) {",
                  "        pm.collectionVariables.set(\"authToken\", authMatch[1]);",
                  "    }",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
